<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
<session-config><session-timeout>60</session-timeout></session-config>
  <servlet>
    <servlet-name>CamList</servlet-name>
    <servlet-class>servlets.CamList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CamEdit</servlet-name>
    <servlet-class>servlets.CamEdit</servlet-class>
  </servlet>
    <servlet>
    <servlet-name>UserList</servlet-name>
    <servlet-class>servlets.UserList</servlet-class>
  </servlet>
	<servlet>
    <servlet-name>UserEdit</servlet-name>
    <servlet-class>servlets.UserEdit</servlet-class>
  </servlet>
  	<servlet>
    <servlet-name>PictureList</servlet-name>
    <servlet-class>servlets.PictureList</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GroupList</servlet-name>
    <servlet-class>servlets.GroupList</servlet-class>
  </servlet> 
  <servlet>
    <servlet-name>GroupEdit</servlet-name>
    <servlet-class>servlets.GroupEdit</servlet-class>
  </servlet>    

  <servlet-mapping>
    <servlet-name>CamList</servlet-name>
    <url-pattern>/camList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CamEdit</servlet-name>
    <url-pattern>/camEdit</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UserList</servlet-name>
    <url-pattern>/userList</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>UserEdit</servlet-name>
    <url-pattern>/userEdit</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>PictureList</servlet-name>
    <url-pattern>/pictureList</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
    <servlet-name>GroupList</servlet-name>
    <url-pattern>/groupList</url-pattern>
  </servlet-mapping>  
   <servlet-mapping>
    <servlet-name>GroupEdit</servlet-name>
    <url-pattern>/groupEdit</url-pattern>
  </servlet-mapping>   
</web-app>